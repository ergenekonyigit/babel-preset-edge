{
  "env": {
    "development": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "modules": false,
            "useBuiltins": "usage",
            "targets": {
              "node": "6.10"
            }
          }
        ]
      ]
    },
    "test": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "useBuiltins": "usage",
            "targets": {
              "node": "6.10"
            }
          }
        ]
      ]
    }
  },
  "plugins": [
    "@babel/plugin-proposal-object-rest-spread"
  ]
}
